     <span style="font-style: italic"><span style="color: #9A1900">//</span></span>
     ok <span style="color: #990000">&amp;=</span> CppAD<span style="color: #990000">::</span><span style="font-weight: bold"><span style="color: #000000">min_nso_linear</span></span><span style="color: #990000">(</span>
          level<span style="color: #990000">,</span> g<span style="color: #990000">,</span> a<span style="color: #990000">,</span> epsilon<span style="color: #990000">,</span> maxitr<span style="color: #990000">,</span> b_in<span style="color: #990000">,</span> x_in<span style="color: #990000">,</span> x_out
